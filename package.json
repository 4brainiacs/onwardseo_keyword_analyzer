{
  "name": "seo-keyword-analyzer",
  "version": "1.2.5",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "prebuild": "rm -rf dist",
    "build": "tsc -p tsconfig.prod.json && vite build",
    "preview": "vite preview --host --port 4173",
    "test": "tsc -p tsconfig.test.json && vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "typecheck": "tsc --noEmit -p tsconfig.prod.json",
    "typecheck:test": "tsc --noEmit -p tsconfig.test.json",
    "clean": "rm -rf dist",
    "update-types": "supabase gen types typescript --project-id xcrumtibxhxgadevxpye > src/lib/database.types.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "2.39.7",
    "@vitejs/plugin-react": "4.2.1",
    "axios": "1.6.7",
    "cheerio": "1.0.0-rc.12",
    "dompurify": "3.0.9",
    "http-status-codes": "2.3.0",
    "jsdom": "24.0.0",
    "lucide-react": "0.294.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.3.3",
    "vite": "5.4.11",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.1.5",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.1",
    "@types/dompurify": "3.0.5",
    "@types/jsdom": "21.1.6",
    "@types/node": "20.10.4",
    "@types/react": "18.2.43",
    "@types/react-dom": "18.2.17",
    "@types/testing-library__jest-dom": "5.14.9",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "autoprefixer": "10.4.16",
    "eslint": "8.55.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "postcss": "8.4.32",
    "supabase": "1.145.4",
    "tailwindcss": "3.3.6",
    "vitest": "1.0.4"
  }
}